{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run unit tests",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build Everything",
      "program": "${workspaceFolder}/src/Octokit.CodeGen/bin/Debug/netcoreapp3.1/Octokit.CodeGen.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Octokit.Tests",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Run convention tests",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Octokit.Tests.Conventions/bin/Debug/netcoreapp3.1/Octokit.Tests.Conventions.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Octokit.Tests.Conventions",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Run integration tests",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Octokit.Tests.Integration/bin/Debug/netcoreapp3.1/Octokit.Tests.Integration.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Octokit.Tests.Integration",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Attach to process",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
